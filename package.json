{
  "name": "xud-socketio",
  "version": "1.0.0",
  "main": "src/index.js",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "start": "ts-node src/index.ts",
    "start:watch": "nodemon --watch src/*.ts --exec ts-node src/index.ts",
    "compile:proto": "grpc_tools_node_protoc --js_out=import_style=commonjs,binary:src/proto --ts_out=src/proto --grpc_out=src/proto --plugin=protoc-gen-ts=node_modules/.bin/protoc-gen-ts -I=proto proto/*.proto proto/google/api/*.proto proto/google/protobuf/*.proto"
  },
  "dependencies": {
    "@types/big.js": "^4.0.5",
    "big.js": "^5.2.2",
    "express": "^4.17.1",
    "google-protobuf": "^3.12.2",
    "grpc": "^1.24.3",
    "socket.io": "^2.3.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.6",
    "@types/node": "^14.0.14",
    "@types/socket.io": "^2.1.8",
    "grpc-tools": "^1.9.0",
    "grpc_tools_node_protoc_ts": "^4.1.0",
    "nodemon": "^2.0.4",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.5"
  }
}
